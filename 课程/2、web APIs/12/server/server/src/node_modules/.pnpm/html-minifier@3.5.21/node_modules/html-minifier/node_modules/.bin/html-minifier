#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/dingwei/Desktop/lesson/二阶段/后三周js/day23/资料/server/src/node_modules/.pnpm/html-minifier@3.5.21/node_modules/html-minifier/node_modules:/Users/dingwei/Desktop/lesson/二阶段/后三周js/day23/资料/server/src/node_modules/.pnpm/html-minifier@3.5.21/node_modules:/Users/dingwei/Desktop/lesson/二阶段/后三周js/day23/资料/server/src/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/dingwei/Desktop/lesson/二阶段/后三周js/day23/资料/server/src/node_modules/.pnpm/html-minifier@3.5.21/node_modules/html-minifier/node_modules:/Users/dingwei/Desktop/lesson/二阶段/后三周js/day23/资料/server/src/node_modules/.pnpm/html-minifier@3.5.21/node_modules:/Users/dingwei/Desktop/lesson/二阶段/后三周js/day23/资料/server/src/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
